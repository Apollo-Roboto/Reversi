<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<OutputType>Library</OutputType>

		<RootNamespace>ReversiBot</RootNamespace>

		<AssemblyName>ApolloRoboto.Reversi</AssemblyName>
		<PackageId>ApolloRoboto.Reversi</PackageId>
		<Version>0.0.1</Version>

		<PackageTitle>ApolloRoboto.Reversi</PackageTitle>
		<Title>ApolloRoboto.Reversi</Title>
		<Description>Library implementation of the Reversi board game.</Description>
		<Authors>Apollo-Roboto</Authors>
		<!-- <PackageProjectUrl>https://github.com/Apollo-Roboto/ApolloRoboto.Reversi</PackageProjectUrl> -->
		<RepositoryUrl>https://github.com/Apollo-Roboto/ApolloRoboto.Constants</RepositoryUrl>
		<PackageTags>Bot;Reversi;Game;Board Game;</PackageTags>
		<Copyright>Copyright Â© 2022 | Apollo-Roboto</Copyright>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageReleaseNotes>README.md</PackageReleaseNotes>
		<!-- <PackageIcon>something.png</PackageIcon> -->
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE" Pack="true" PackagePath="" />
		<None Include="..\README.md" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="YamlDotNet" Version="11.2.1" />
	</ItemGroup>

</Project>
